    "@material-ui/styles": "^4.9.0",
    "@netlify/plugin-lighthouse": "^1.4.1",
    "@reach/router": "^1.3.3",
    "emotion-reset": "^2.0.7",
    "gatsby": "^2.3.29",
    "gatsby-image": "^2.0.40",
    "gatsby-plugin-emotion": "^4.0.6",
    "gatsby-plugin-extract-schema": "^0.0.5",
